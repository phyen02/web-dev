<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar-collapse collapse">
        <ul class="nav navbar-nav">
            <li>@Html.ActionLink("ViewResult", "TestViewResult", "Product")</li>
            <li>@Html.ActionLink("PartialViewResult", "TestPartialViewResult", "Product")</li>
            <li>@Html.ActionLink("EmptyResult", "TestEmptyResult", "Product")</li>
            <li>@Html.ActionLink("RedirectResult ", "TestRedirectResult", "Product")</li>
            <li>@Html.ActionLink("JsonResult", "TestJsonResult", "Product")</li>
            <li>@Html.ActionLink("JavaScriptResult", "TestJavaScriptResult", "Product")</li>
            <li>@Html.ActionLink("ContentResult", "TestContentResult", "Product")</li>
            <li>@Html.ActionLink("FileContentResult ", "TestFileContentResult", "Product")</li>
            <li>@Html.ActionLink("FileStreamResult", "TestFileStreamResult", "Product")</li>
            <li>@Html.ActionLink("FilePathResult", "TestFilePathResult", "Product")</li>
        </ul>
    </div>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
